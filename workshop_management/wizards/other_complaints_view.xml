<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!--    Other complaints wizard view form-->
    <record id="other_complaints_wizard_view_form" model="ir.ui.view">
        <field name="name">other.complaints.wizard.view.form</field>
        <field name="model">other.complaints</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <group>
                            <field name="other_complaints"/>
                            <field name="appointment"/>
                        </group>
                    </group>
                    <footer>
                        <button name="confirm_complaints" string="CONFIRM"
                                type="object" class="oe_highlight"/>
                        <button string="Cancel order"
                                special="cancel" class="oe_highlight"/>
                    </footer>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
