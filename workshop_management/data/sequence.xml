<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
<!--        Appointment sequence code-->
        <record id="appointment_sequence" model="ir.sequence">
            <field name="name">Appointment sequence</field>
            <field name="code">workshop.appointment.sequence</field>
            <field name="prefix">WSA</field>
            <field name="padding">2</field>
            <field name="company_id" eval="False"/>
        </record>
<!--        Work order sequence code-->
        <record id="work_order_sequence" model="ir.sequence">
            <field name="name">Work order sequence</field>
            <field name="code">workshop.order.sequence</field>
            <field name="prefix">WO</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>